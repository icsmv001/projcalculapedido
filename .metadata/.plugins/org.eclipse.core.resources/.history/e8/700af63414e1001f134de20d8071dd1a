@Service
public class ShippingService {
	
	 	
		public double shipment(double amount) {
		//	return amount * 0.2 ;  
			
			  if(amount != null) {
				        200;
				       } else {
				         500;
				       }
				       return amount;
				     }	
			
			
			
			
			
			
	}
}
