package com.devsuperior.order.entities;

public class Order {
	
	 private Integer code;
	 private double  basic;
	 private double  discount;
	 
	
	 public Order() {
		
	 }
		
	 
	 public Order(Integer code, double basic, double discount) {
		this.code = code;
		this.basic = basic;
		this.discount = discount;
	}
	
	
	
	

}
